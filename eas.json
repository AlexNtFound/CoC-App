{
  "cli": {
    "version": ">= 7.8.6"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "default"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "resourceClass": "default"
      }
    },
    "production": {
      "ios": {
        "distribution": "store",
        "resourceClass": "default"
      },
      "android": {
        "distribution": "store"
      }
    }
  }
}